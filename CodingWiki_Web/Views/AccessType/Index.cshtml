@model IEnumerable<Practice5_DataAccess.Data.AccessType>

<br />
<br />
<div class="border p-4 bg-white">
    <div class="row">
        <div class="col-10">
            <h2 class="text-info">Choose A Data Acess Type</h2>
        </div>
    </div>
    <br />
    <div>
        @if (Model.Count() > 0)
        {
            <table class="table table-striped border">
                <tr class="table-secondary">
                    <th>
                        Type
                    </th>
                    <th>
                        Currently in use?
                    </th>
                    <th></th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.obj_choice
                        </td>
                        <td>
                            @item.obj_isSet
                        </td>
                        <td>
                            <a class="btn btn-primary text-white" asp-action="Set" asp-route-Id="@item.obj_id">
                                Set
                            </a>
                        </td>
                    </tr>
                }
            </table>
        }
        else
        {
            <p>No Access Types exist...</p>
        }
    </div>
</div>



